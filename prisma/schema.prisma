generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Sweet {
  id        Int      @id @default(autoincrement())
  name      String
  category  String
  price     Decimal  @db.Decimal(10,2)
  quantity  Int

  @@index([category])
  @@unique([name, category])
}